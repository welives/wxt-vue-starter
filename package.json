{
  "name": "wxt-vue-starter",
  "description": "A simple wxt starter using vue, unocss and shadcn",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "keywords": [
    "vite",
    "vue3",
    "wxt",
    "unocss",
    "shadcn-vue"
  ],
  "author": {
    "name": "welives",
    "url": "https://github.com/welives"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:welives/wxt-vue-starter.git"
  },
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "vue-tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "@wxt-dev/storage": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.456.0",
    "radix-vue": "^1.9.9",
    "tailwind-merge": "^2.5.4",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/chrome": "^0.0.280",
    "@types/node": "^22.9.0",
    "@unocss/eslint-plugin": "^0.64.1",
    "@unocss/postcss": "^0.64.0",
    "@unocss/reset": "^0.64.0",
    "@wxt-dev/module-vue": "^1.0.1",
    "eslint": "^9.15.0",
    "esno": "^4.8.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "unocss": "^0.64.0",
    "unocss-preset-animations": "^1.1.0",
    "unocss-preset-shadcn": "^0.3.1",
    "vue-tsc": "^2.1.10",
    "wxt": "^0.19.13",
    "@antfu/eslint-config": "^3.10.0"
  }
}
